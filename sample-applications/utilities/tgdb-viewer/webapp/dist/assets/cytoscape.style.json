[
  {
    "selector": "node",
    "style": {
      "font-family": "Source Sans Pro",
      "width": "32px",
      "height": "32px",
      "background-color": "#D5E6FF",
      "border-color": "#FFF",
      "border-width": 2,
      "content": "data(name)"
    }
  },
  {
    "selector": "node#u",
    "style": {
      "background-color": "blue"
    }
  },
  {
    "selector": "edge",
    "style": {
      "width": 2,
      "line-color": "#B6B6B6",
      "target-arrow-color": "#B6B6B6",
      "source-arrow-color": "#B6B6B6",
      "curve-style": "bezier",
      "target-arrow-shape": "vee",
      "content": "data(name)"
    }
  },
  {
    "selector": "edge#pu",
    "style": {
      "source-arrow-color": "#B6B6B6",
      "target-arrow-shape": "vee",
      "mid-target-arrow-shape": "vee",
      "source-arrow-shape": "vee"
    }
  },


  {
    "selector": ".eh-handle",
    "style": {
      "background-color": "red",
      "width": 12,
      "height": 12,
      "shape": "ellipse",
      "overlay-opacity": 0,
      "border-width": 1,
      "border-opacity": 0
    }
  },
  {
    "selector": ".eh-hover",
    "style": {
      "background-color": "red"
    }
  },
  {
    "selector": ".eh-source",
    "style": {
      "border-width": 2,
      "border-color": "red"
    }
  },
  {
    "selector": ".eh-target",
    "style": {
      "border-width": 2,
      "border-color": "red"
    }
  },
  {
    "selector": ".eh-preview, .eh-ghost-edge",
    "style": {
      "background-color": "red",
      "line-color": "red",
      "target-arrow-color": "red"
    }
  },
  {
    "selector": ".node-on-hover-highlight",
    "style": {
      "background-color": "#569AFF",
      "border-color": "#D5E6FF",
      "border-width": 2,
      "border-opacity": 1
    }
  },
  {
    "selector": ".node-on-hover-edge-highlight",
    "style": {
      "line-color": "#569AFF",
      "target-arrow-color": "#569AFF",
      "mid-target-arrow-color": "#569AFF",
      "source-arrow-color": "#569AFF"
    }
  },
  {
    "selector": ".node-on-select-highlight",
    "style": {
      "height": "32px",
      "width": "32px",
      "background-color": "#569AFF",
      "border-color": "#D5E6FF",
      "border-width": "8px"
    }
  },
  {
    "selector": ".node-on-select-edge-highlight",
    "style": {
      "line-color": "#0051AC",
      "target-arrow-color": "#0051AC",
      "mid-target-arrow-color": "#0051AC",
      "source-arrow-color": "#0051AC"
    }
  },
  {
    "selector": ".node-bigger",
    "style": {
      "background-color": "blue",
      "width": 30,
      "height": 30
    }
  },
  {
    "selector": ".attribute",
    "style": {
      "font-family": "Source Sans Pro",
      "background-color": "#FF8456",
      "height": "16px",
      "width": "16px",
      "border-width": 0
    }
  },
  {
    "selector" : ":parent",
    "style": {
      "background-color" : "#FFF5EC",
      "background-opacity": 0.7
    }
  },
  {
    "selector" : "node.cy-expand-collapse-collapsed-node",
    "style": {
      "background-image" : "./assets/images/send.svg",
      "background-fit": "cover",
      "background-opacity": 0.7
    }
  }
]
