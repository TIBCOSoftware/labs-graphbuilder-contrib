{
	"name": "tibco-sql",
	"title": "SQL Compatible database connection",
	"author": "TIBCO Software Inc.",
	"type": "flogo:connector",
	"version": "1.0.0",
	"display": {
		"description": "This is connector for SQL compatible database",
		"category": "GraphBuilder_SQL",
		"visible": true,
		"smallIcon": "sqldb.png"
	},
	"ref": "github.com/TIBCOSoftware/GraphBuilder_SQL/connector/sqldb",
