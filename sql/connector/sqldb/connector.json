		},
		{
			"name": "password",
			"type": "string",
			"required": false,
			"display": {
				"name": "Password",
				"description": "User password",
				"type": "password"
			}
		},
		{
			"name": "resource",
			"type": "string",
			"required": false,
			"display": {
				"name": "Resource Name",
				"description": "Resource name for the event service",
				"type": "string"
			}
		}
