	"settings": [
		{
			"name": "name",
			"type": "string",
			"required": true,
			"display": {
			"name": "Connection Name",
			"description": "Name of the connection"
			}
		},
		{
			"name": "description",
			"type": "string",
			"display": {
				"name": "Description",
				"description": "Connection description"
			}
		},
		{
			"name": "vendor",
			"type": "string",
			"required": true,
			"display": {
				"name": "Vendor ID",
				"description": "The vendor identifier",
				"type": "string"
			},
        		"allowed":["mysql"]

		},
		{
			"name": "url",
			"type": "string",
			"required": true,
			"display": {
				"name": "Server URL",
